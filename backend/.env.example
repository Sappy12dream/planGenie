# Supabase
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# OpenAI
OPENAI_API_KEY=

# Resend (Email Notifications)
# Get your API key from https://resend.com (Free tier: 100 emails/day)
RESEND_API_KEY=

# App Settings
ENVIRONMENT=development
DEBUG=True
FRONTEND_URL=http://localhost:3004

# Sentry Configuration (Error Tracking & Monitoring)
# Get your DSN from https://sentry.io after creating a project
SENTRY_DSN=

# Sentry Performance Monitoring
# 1.0 = 100% of transactions (recommended for development)
# 0.1 = 10% of transactions (recommended for production to reduce costs)
SENTRY_TRACES_SAMPLE_RATE=1.0

# Sentry Profiling
# 1.0 = 100% of sampled transactions (recommended for development)
# 0.1 = 10% of sampled transactions (recommended for production)
SENTRY_PROFILES_SAMPLE_RATE=1.0
